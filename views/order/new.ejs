<!-- Form for order -->
<div class="order">
    <form action="/order/new" method="POST" class="oform">
      <h3 class="welcome-login-message">Welcome</h3>
      <p class="welcome-login-sign-in">Start a order</p>
      <div class="form-group">
        <label for="orderName"></label>
        <input type="text" class="form-control" id="name" name="orderName" placeholder="Order Name: ">
      </div>
      <div class="form-group">
        <label for="orderFrom"></label>
        <input type="text" class="form-control" id="orderFrom" name="orderFrom" placeholder="Order From: ">
      </div>
      <div class="form-group">
        <label for="orderTo"></label>
        <input type="text" class="form-control" id="orderTo" name="orderTo" placeholder="Order To: ">
      </div>
